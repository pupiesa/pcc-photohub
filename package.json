{
  "name": "pcc-photohub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ui": "next dev",
    "mongo": "node --env-file=.env ./photobootAPI/mongo-api/mongoAPI.js",
    "nextcloud": "node --env-file=.env ./photobootAPI/nextcloud-api/nextcloudAPI.js",
    "dev": "concurrently -n mongoDB,nextcloud,ui -c green,blue,magenta \"npm:mongo\" \"npm:nextcloud\" \"npm:ui\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "install:all": "npm --prefix ./photobootAPI/mongo-api i && npm --prefix ./photobootAPI/nextcloud-api i"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dropdown-menu": "^0.1.1",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.12",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
